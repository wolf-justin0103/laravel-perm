{
    "name": "spatie/laravel-permission",
    "description": "Permission handling for Laravel 5.1",
    "keywords": [
        "spatie",
        "laravel-permission"
    ],
    "homepage": "https://github.com/spatie/laravel-permission",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "illuminate/auth": "^5.1.11",
        "illuminate/database": "^5.1.11",
        "illuminate/support": "^5.1.11"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Permission\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Permission\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
